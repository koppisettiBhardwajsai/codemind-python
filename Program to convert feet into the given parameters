feet=int(input("Enter feet:"))
opt=int(input("enter choice 1:inches 2:yards 3:miles 4:millimeters 5.centimeter 6.meters
7.kilometers"))
if opt==1:
    print(round(feet*12,3))
elif opt==2:
    print(round(feet*0.333,3))
elif opt==3:
    print(round(feet*0.000189,3))
elif opt==4:
    print(round(feet*304.8,3))
elif opt==5:
    print(round(feet*30.48,3))
elif opt==6:
    print(round(feet*0.305,3))
elif opt==7:
    print(round(feet*0.000305,3))
